@using System.Web.Optimization

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="Expires" content="0" />
    <meta http-equiv="Last-Modified" content="0" />
    <meta http-equiv="Cache-Control" content="no-cache, mustrevalidate" />
    <meta http-equiv="Pragma" content="no-cache" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/Bootstrap")

    <style type="text/css">


        table {
            border: none;
            width: 100%;
            border-collapse: collapse;
        }
    </style>


</head>
<body>

    <!-- ===========================================================================================================================-->
    <!-- NAVEGADOR                                                                                                                  -->
    <!-- ===========================================================================================================================-->
    <nav class="navbar navbar-default menu_opciones">
        <div class="container-fluid" style="background-color:#0d3c6c">
            <div class="navbar-header">
                <a class="navbar-brand enlaces_menu">
                    <img style="max-width: 215px; margin-top: 6px; margin-left: 0px; height: 50px" alt="Logo"
                         src='~/Imagenes/way_blanco_letras.png' />
                </a>
            </div>
            <div class="collapse navbar-collapse" id="myNavbar">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </nav>
    <!-- ===========================================================================================================================-->
    <!-- FIN NAVEGADOR                                                                                                              -->
    <!-- ===========================================================================================================================-->

    <div class="container-fluid body-content">
        @RenderBody()
    </div>

    @Scripts.Render("~/Scripts/jquery")
    @Scripts.Render("~/Scripts/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>


